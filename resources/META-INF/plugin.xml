<idea-plugin version="2">
    <id>com.kotcrab.quicklaunch</id>
    <name>Quick Launch</name>
    <version>1.0</version>
    <vendor url="https://www.kotcrab.com">Kotcrab</vendor>
    <!--<vendor email="" url="http://www.kotcrab.com">Kotcrab</vendor>-->

    <description><![CDATA[
      Running launch configurations simplified
    ]]></description>

    <change-notes><![CDATA[
      1.0
       - Initial release
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="141.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Quick Launch" anchor="right" secondary="true" icon="AllIcons.Toolwindows.ToolWindowFavorites" factoryClass="com.kotcrab.quicklaunch.QuickLaunchToolWindowFactory"/>
    </extensions>
</idea-plugin>
